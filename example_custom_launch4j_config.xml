<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>MyApplicationJar32bit.jar</jar>
  <outfile>MyApplicationExecutable32bit.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>path\to\my\applicatins\icon\icon.ico</icon>
  <jre>
    <path></path>
    <bundledJre64Bit>false</bundledJre64Bit>
    <bundledJreAsFallback>false</bundledJreAsFallback>
    <jreDownloadUrl>http://download.oracle.com/otn-pub/java/jdk/8u65-b17/jre-8u65-windows-i586.exe</jreDownloadUrl>
    <downloadAndInstallJre>true</downloadAndInstallJre>
    <jreDownloadCookie>gpw_e24=http%3A%2F%2Fwww.oracle.com%2Ftechnetwork%2Fjava%2Fjavase%2Fdownloads%2Fjre8-downloads-2133155.html; s_sq=%5B%5BB%5D%5D; oraclelicense=accept-securebackup-cookie;</jreDownloadCookie>
    <minVersion>1.8.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>32</runtimeBits>
  </jre>
  <splash>
    <file>path\to\splash\image\splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>900</timeout>
    <timeoutErr>false</timeoutErr>
  </splash>
</launch4jConfig>